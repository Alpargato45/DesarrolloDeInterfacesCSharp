<UserControl x:Class="PrimerControlUsuarioNET.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrimerControlUsuarioNET"
             mc:Ignorable="d" Height="76" Width="164">
    <Grid Margin="-42,-42,0,0" Width="205" Height="112" HorizontalAlignment="Left" VerticalAlignment="Top">
        <TextBox x:Name="textBox" Height="35" Margin="50,70,0,0" TextWrapping="Wrap" Width="142" TextChanged="textBox_TextChanged"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Height="25" Margin="50,45,0,0" VerticalAlignment="Top" Width="142" Foreground="Red"/>
        <Label x:Name="labelCuentaNumero" Content="0" HorizontalAlignment="Left" Height="25" Margin="133,45,0,0" VerticalAlignment="Top" Width="62" Foreground="Green" Visibility="Hidden"/>
        <Label x:Name="labelBarra" Content="/" HorizontalAlignment="Left" Height="25" Margin="152,45,0,0" VerticalAlignment="Top" Width="40" Foreground="Green" Visibility="Hidden"/>
        <Label x:Name="labelMaximoNumero" Content=" " HorizontalAlignment="Left" Height="25" Margin="164,45,0,0" VerticalAlignment="Top" Width="28" Foreground="Green" Visibility="Hidden"/>

    </Grid>
</UserControl>
